@model IEnumerable<Ros.WebApplication.Models.ViewModels.ResultViewModels.ResultIndexViewModel>

@{
    ViewBag.Title = "Results Index";
}

<h2>Results Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Rank)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Points)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Distance)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CalculatedTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CalculatedDistance)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Remark)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EntryNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EntryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Location)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Class)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Rank)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Points)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Distance)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CalculatedTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CalculatedDistance)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remark)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Entry.EntryNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Entry.EntryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RaceEvent.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RaceEvent.Location)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RaceEvent.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RaceEvent.Class)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RaceEvent.Type)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}
</table>